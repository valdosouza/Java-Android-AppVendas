<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.setes.setesvendas.app">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".pedidovenda.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".pedidovenda.ConfigActivity"
            android:label="@string/title_activity_config" />
        <activity
            android:name=".pedidovenda.AddAddressActivity"
            android:label="@string/title_activity_add_address" />
        <activity
            android:name=".pedidovenda.AddOrderSaleItemActivity"
            android:label="@string/title_activity_add_order_sale_item" />
        <activity
            android:name=".pedidovenda.AddPhoneActivity"
            android:label="@string/title_activity_add_phone" />
        <activity
            android:name=".pedidovenda.CustomerActivity"
            android:label="@string/title_activity_customer" />
        <activity
            android:name=".pedidovenda.ListAddressActivity"
            android:label="@string/title_activity_list_address" />
        <activity
            android:name=".pedidovenda.ListOrderSaleItemActivity"
            android:label="@string/title_activity_list_order_sale_item" />
        <activity
            android:name=".pedidovenda.ListPhoneActivity"
            android:label="@string/title_activity_list_phone" />
        <activity
            android:name=".pedidovenda.OrderActivity"
            android:label="@string/title_activity_order" />
        <activity
            android:name=".pedidovenda.ReceiveWsActivity"
            android:label="@string/title_activity_receive_ws" />
        <activity
            android:name=".pedidovenda.SearchCustomerActivity"
            android:label="@string/title_activity_search_customer" />
        <activity
            android:name=".pedidovenda.SearchOrderActivity"
            android:label="@string/title_activity_search_order" />
        <activity
            android:name=".pedidovenda.SearchProductActivity"
            android:label="@string/title_activity_search_product" />
        <activity
            android:name=".pedidovenda.SendWsActivity"
            android:label="@string/title_activity_send_ws" />
        <activity
            android:name=".pedidovenda.SendWsActivityByOne"
            android:label="@string/title_activity_send_ws_activity_by_one" />
        <activity
            android:name=".pedidovenda.OperationDatabase"
            android:label="@string/title_activity_operation_database" />
        <activity
            android:name=".pedidovenda.OrderCopyItemListActivity"
            android:label="@string/title_activity_order_copy_item_list"></activity>
    </application>

</manifest>
